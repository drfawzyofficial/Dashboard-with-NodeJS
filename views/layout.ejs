<!doctype html>
<html lang="ar">

<head>

  <meta charset="utf-8" />
  <title>Dashboard | Skote - Nodejs Admin & Dashboard Template</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
  <meta content="Themesbrand" name="author" />
  <!-- App favicon -->
  <link rel="shortcut icon" href="/assets/images/favicon.ico">

  <%- HeaderCss %>
  <%- include ('./Partials/HeaderStyle')  %>

</head>

<body data-sidebar="dark">

  <!-- Begin page -->
  <div id="layout-wrapper">

    <%- include ('./Partials/Header')  %>
    <%- include ('./Partials/Sidebar')  %>

    <div class="main-content">
      <div class="page-content">
        <div class="container-fluid">

          <%- breadcrumb %>
          <div class="row">
            <div class="col-12">
                <%- include('./Partials/alert') %>
            </div>
          </div>
          <%- body %>
        </div>
      </div>

      <%- include ('./Partials/Footer') %>
    </div>
  </div>

  <%- include ('./Partials/Rightbar')  %>
  <!-- Notification -->
  <div class="notificationSuccess text-white shadow-lg p-3"></div>
  <div class="notificationError text-white shadow-lg p-3"></div>
  <!-- Notification -->
  <%- include ('./Partials/FooterScript')  %>

  <%- FooterJs %>

  <script src="/assets/js/app.js"></script>

  <%- BottomJs %>
</body>

</html>